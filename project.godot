; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Cosmic Time Trials"
config/description="3D space racing game. Formerly known as; CQB Simulator, Lunar-Front."
run/main_scene="res://Scenes/Scrapyard Map/scrapyard_map_b.tscn"
config/features=PackedStringArray("4.1", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://Graphics/loading_texture.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://Graphics/Export Graphics/icon.png"
config/windows_native_icon="res://Graphics/Export Graphics/icon.ico"

[autoload]

game_data="*res://Data/game_data.gd"
map_data="*res://Data/map_data.gd"
global_data="*res://Data/global_data.gd"
SilentWolf="*res://addons/silent_wolf/SilentWolf.gd"
settings="*res://Data/settings.gd"

[display]

window/size/viewport_width=1600
window/size/viewport_height=900
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor_plugins]

